ORG 0000H
		LJMP MAIN
		ORG 0100H
MAIN:	MOV A,#00H					
LOOP:   JB P3.3,LOOP		
	    LCALL	DEL			
		JB P3.3,LOOP		
LOOP1:  JNB P3.3,LOOP1
	    LCALL DEL	
		INC A				
		CPL A				
		MOV P1,A			
		LCALL	DEL
	    CPL A
	    LJMP LOOP
DEL: 	MOV R7,#20			
DEL1:	MOV R6,#249
DEL2:	DJNZ R6,DEL2
		DJNZ R7,DEL1
		RET
END